@using System.Collections.Generic
@using AspNetCoreConsoleApp.Contracts.Entities
@model IEnumerable<User>
<html>
    <body>
        <p>Sup, Earth.</p>
        <table>
            <thead>
                <tr>
                    <th>UserID</th>
                    <th>Username</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Password</th>
                </tr>
            </thead>
            <tbody>
            @foreach(var user in Model)
            {
                <tr>
                    <td>@user.UserId</td>
                    <td>@user.Username</td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.Password</td>
                </tr>
            }
        </table>
    </body>
</html>